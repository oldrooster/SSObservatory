# Azure AD / Microsoft Entra configuration
AZURE_TENANT_ID=
AZURE_CLIENT_ID=
AZURE_CLIENT_SECRET=

# PostgreSQL connection settings
PGHOST=
PGPORT=5432
PGDATABASE=
PGUSER=
PGPASSWORD=
# Optional: prefer "require" when available
PGSSLMODE=

# Optional tuning knobs
LOOKBACK_DAYS=30
GRAPH_PAGE_SIZE=100
# Graph filter used to fetch enterprise apps; defaults to the safest minimal filter
SERVICE_PRINCIPAL_FILTER=servicePrincipalType eq 'Application'
# Local post-processing filters
EXCLUDE_HIDE_APP_TAG=true
EXCLUDE_OWNER_ORGANIZATION_IDS=f8cdef31-a31e-4b4a-93e4-5f571e91255a
EXCLUDE_PUBLISHERS=Microsoft
